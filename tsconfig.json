{
  "compilerOptions": {
    "allowUnusedLabels": true,
    "allowJs": false,
    "lib": [
      "es2016",
      "esnext.asynciterable"
    ],
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "esModuleInterop": true
  }
}